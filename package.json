{
  "name": "@1fe/sample-widget",
  "version": "0.1.2",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "1fe-cli --trace --environment integration dev",
    "build:widget": "1fe-cli --trace --environment integration build",
