  "dependencies": {
    "@1fe/shell": "0.0.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.1.0",
    "@reduxjs/toolkit": "1.9.1",
    "@textea/json-viewer": "^4.0.1",
    "@types/systemjs": "^6.15.1",
    "antd": "^5.22.4",
    "react": "^18.3.1",
